      "assetsDir": "assets",
      "indexHtml": "index.html"
    },
    "ios": {
      "bundleId": "com.ckizzy1.restexpress",
      "appName": "ZiShop",
      "version": "1.0.0",
      "buildNumber": "1",
      "teamId": "",
      "provisioningProfile": "",
      "certificate": ""
    },
    "android": {
      "packageName": "com.ckizzy1.restexpress",
      "appName": "ZiShop",
      "versionCode": 1,
      "versionName": "1.0.0",
      "minSdkVersion": 22,
      "targetSdkVersion": 33,
      "compileSdkVersion": 33
