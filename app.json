{
  "expo": {
    "name": "ZiShop",
    "slug": "rest-express",
    "version": "1.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./client/src/assets/images/logos/icon-primary.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./client/src/assets/images/logos/logo-primary.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "b71fd963-130f-4efb-879c-ac65a7fd5359"
      }
    },
    "ios": {
      "bundleIdentifier": "com.startindev.zishop",
      "buildNumber": "1",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "66.29.155.41": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "TLSv1.0"
            }
          }
        }
      }
    },
    "plugins": [],
    "web": {
      "bundler": "metro"
    }
  }
}
